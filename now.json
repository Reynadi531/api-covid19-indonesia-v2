{
  "version": 2,
  "name": "COIVD-19 API INDONESIA v2",
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "app.js"
    }
  ],
  "regions": [
      "sin1"
  ],
  "alias": [
    "indonesiapicovid19-v2"
  ],
  "headers": [
    {
      "source": "app.js",
      "headers": [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=60, stale-while-revalidate"
        }
      ]
    }
  ]
}
