{
  "version": 2,
  "name": "COIVD-19 API INDONESIA v2",
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "app.js"
    }
  ],
  "regions": [
      "sin1"
  ],
  "headers": [
    {
      "key": "Cache-Control",
      "value": "public, s-maxage=60, stale-while-revalidate"
    },
    {
      "key": "Access-Control-Allow-Origin",
        "value": "*"
    }
  ],
  "alias": [
    "indonesiapicovid19-v2"
  ]
}
